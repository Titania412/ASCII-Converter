import java.util.*;

public class Convert {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int code;
		
		System.out.println("Enter an ASCII code: ");
		code = sc.nextInt();
		
		System.out.println("The character for ASCII code " + code + " is " + (char)code);
		
		
	}

}
